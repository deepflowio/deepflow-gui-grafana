{
  "name": "metaflow-gui-grafana",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "./*"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "b:apptrace": "pnpm run --filter metaflow-apptrace-panel build",
    "b:querier": "pnpm run --filter metaflow-querier-datasource build",
    "b:topo": "pnpm run --filter metaflow-topo-panel build",
    "b-dev:apptrace": "pnpm run --filter metaflow-apptrace-panel builddev",
    "b-dev:querier": "pnpm run --filter metaflow-querier-datasource builddev",
    "b-dev:topo": "pnpm run --filter metaflow-topo-panel builddev",
    "b:all": "pnpm run b:apptrace && pnpm run b:querier && pnpm run b:topo"
  },
  "dependencies": {
    "@grafana/data": "^8.5.6",
    "@grafana/runtime": "^8.5.6",
    "@grafana/toolkit": "^8.5.6",
    "@grafana/ui": "^8.5.6",
    "lodash": "^4.17.21",
    "metaflow-vis-js": "^0.0.9",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "prettier": "^2.7.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
